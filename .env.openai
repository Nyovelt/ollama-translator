# Example: OpenAI as default configuration
NODE_ENV=production
NEXT_PUBLIC_DEFAULT_CONFIG_ID=openai

# OpenAI Configuration (will be the default)
NEXT_PUBLIC_CUSTOM_LLM_NAME=OpenAI GPT-4
NEXT_PUBLIC_CUSTOM_API_URL=https://api.openai.com/v1
NEXT_PUBLIC_CUSTOM_MODEL=gpt-4
NEXT_PUBLIC_CUSTOM_API_KEY=your_openai_api_key_here

# Fallback Ollama config
NEXT_PUBLIC_DEFAULT_API_URL=http://localhost:11434
NEXT_PUBLIC_DEFAULT_MODEL=llama3.1
